<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Quote_Name_Character_limit</fullName>
    <active>true</active>
    <errorConditionFormula>LEN( Name ) &gt; 80</errorConditionFormula>
    <errorMessage>Quote Name can not be more than 80 characters.</errorMessage>
</ValidationRule>
