<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Restrict_campaign_for_cancelled_contract</fullName>
    <active>false</active>
    <errorConditionFormula>AND(ISNEW(),  ISPICKVAL(Contract__r.Contract_Status__c , &apos;Cancelled&apos;)  )</errorConditionFormula>
    <errorMessage>You do not have a subscription to support this date. Please contact your Client Account Manager.</errorMessage>
</ValidationRule>
